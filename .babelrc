{
  "presets": ["next/babel"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true
      }
    ],
    ["inline-react-svg"],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "constants": "./constants",
          "context": "./context",
          "images": "./images",
          "pages": "./pages",
          "themes": "./themes",
          "utils": "./utils"
        }
      }
    ]
  ]
}
